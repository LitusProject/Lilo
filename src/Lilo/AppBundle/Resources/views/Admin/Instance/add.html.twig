{% extends '@LiloApp/base.html.twig' %}

{% block content %}
    <div class="section">
        <div class="section-header">
            <h1>Instances</h1>
        </div>

        <div class="row">
            <form class="form-horizontal" action="" method="post" {{ form_enctype(instanceForm) }}>
                {{ form_widget(instanceForm, {'form_type': 'horizontal'}) }}
                <div class="form-actions">
                    <input type="submit" name="add" class="btn btn-primary" value="&raquo; Add" />
                </div>
            </form>
        </div>
    </div>
{% endblock %}
